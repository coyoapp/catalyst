{
  "name": "@haiilo/catalyst-react",
  "version": "0.7.0",
  "description": "React wrapper for Catalyst Design System",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/ionic.git"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "compile": "npm run tsc",
    "tsc": "tsc -p .",
    "rollup": "rollup -c",
    "clean": "rm -rf ./dist",
    "reset": "rm -rf ./dist ./node_modules"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/jest": "23.3.9",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "jest": "^23.0.0",
    "jest-dom": "^3.0.2",
    "np": "^3.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^3.3.4000"
  },
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  }
}
