{
  "color": {
    "ui": {
      "background": {
        "canvas": {
          "$type": "color",
          "$value": "{color.base.neutral.100.value}",
          "cssProp": "bg"
        },
        "surface": { "$type": "color", "$value": "{color.base.white.value}" },
        "surfaceInverted": {
          "$type": "color",
          "$value": "{color.base.neutral.700.value}"
        },
        "muted": {
          "$type": "color",
          "$value": "{color.base.neutral.100.value}"
        },
        "input": { "$type": "color", "$value": "{color.base.white.value}" },
        "inputAutofill": { "$type": "color", "$value": "#e8f0fe" },
        "skeleton": {
          "$type": "color",
          "$value": "{color.base.neutral.200.value}"
        },
        "skeletonHighlight": {
          "$type": "color",
          "$value": "{color.base.neutral.300.value}"
        },
        "tooltip": {
          "$type": "color",
          "$value": "{color.base.black.value}",
          "cssProp": "bg-tooltip"
        },
        "backdrop": {
          "$type": "color",
          "$value": "{color.base.neutral.700}",
          "cssProp": "bg-backdrop"
        }
      },
      "border": {
        "light": {
          "$type": "color",
          "$value": "{color.base.neutral.100.value}",
          "cssProp": "border-color-light"
        },
        "default": {
          "$type": "color",
          "$value": "{color.base.neutral.200.value}",
          "cssProp": "border-color"
        },
        "dark": {
          "$type": "color",
          "$value": "{color.base.neutral.300.value}",
          "cssProp": "border-color-dark"
        },
        "focus": {
          "$type": "color",
          "$value": "#0071ff",
          "cssProp": "border-color-focus"
        },
        "quote": {
          "$type": "color",
          "$value": "{color.base.neutral.400.value}"
        }
      },
      "font": {
        "head": {
          "$type": "color",
          "$value": "{color.ui.font.body.value}",
          "cssProp": "font-color-head"
        },
        "headInverted": {
          "$type": "color",
          "$value": "{color.ui.font.bodyInverted.value}"
        },
        "body": {
          "$type": "color",
          "$value": "{color.base.black.value}",
          "cssProp": "font-color-body"
        },
        "bodyInverted": {
          "$type": "color",
          "$value": "{color.base.white.value}"
        },
        "mono": {
          "$type": "color",
          "$value": "{color.base.black.value}",
          "cssProp": "font-color-mono"
        },
        "monoInverted": {
          "$type": "color",
          "$value": "{color.base.white.value}"
        },
        "muted": {
          "$type": "color",
          "$value": "{color.base.neutral.400.value}",
          "cssProp": "font-color-muted"
        },
        "placeholder": {
          "$type": "color",
          "$value": "{color.base.neutral.500.value}"
        },
        "hint": {
          "$type": "color",
          "$value": "{color.base.neutral.500.value}"
        },
        "quote": {
          "$type": "color",
          "$value": "{color.base.neutral.500.value}",
          "cssProp": "font-color-quote"
        },
        "tooltip": {
          "$type": "color",
          "$value": "{color.base.white.value}",
          "cssProp": "font-color-tooltip"
        }
      }
    }
  }
}
